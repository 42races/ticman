<div class="row">
  <div class="col s12 m12">
    <div class="search-wrapper card">
      <form id="tic-search-form" action="<%= search_dashboard_index_path %>" method="get">
        <input id="search" autocomplete="off" placeholder="Search here" name="query">
        <i id="tic-search-icon" class="material-icons">search</i>
      </form>
    </div>
  </div>
</div>

<div class="row">
  <div class="col s12 m12">
    <div class="chip">Total <%= @tics.count %> events found. </div>
    <%= paginate @tics %>
    <ul class="collection">
      <% @tics.each do |tic| %>
        <li class="collection-item"><%= display(tic) %></li>
      <% end %>
    </ul>
    <%= paginate @tics %>
  </div>
</div>

<div class="row">
  <div class="col s12 m3">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">Card Title</span>
        <p>I am a very simple card. I am good at containing small bits of information.
        I am convenient because I require little markup to use effectively.</p>
      </div>
      <div class="card-action">
        <a href="#">This is a link</a>
        <a href="#">This is a link</a>
      </div>
    </div>
  </div>
</div>